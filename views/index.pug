extends layout

block content
  .nav
     .nav-wrapper
       ul#nav-mobile.right.hide-on-med-and-down
       li
       a(href='./getAllHeroes') All Heroes

  h1= title
  p Add #{title}
  form(action='saveData' method='get')
   .row
    .input-field.col.s6
      label(for='superhero') Super Hero
      input#superhero.validate( type='text', name='superHero')
   .row
    .input-field.col.s6
      label(for='publisher') Publisher
      input#publisher.validate( type='text', name='publisher')
   .row
    .input-field.col.s6
      label(for='alterEgo') Alter Ego
      input#alterEgo.validate( type='text', name='alterEgo')
   .row
    .input-field.col.s6
      label(for='firstAppearance') First Appearance
      input#firstAppearance.validate( type='text', name='firstAppearance')
    .row
    .input-field.col.s6
      label(for='character') Character
      input#character.validate( type='text', name='character')
      

   .row
    button.btn(type='submit') Go